{
  "name": "ngx-delete-confirm",
  "version": "0.0.3",
  "description": "",
  "main": "./dist/umd/ngx-delete-confirm.js",
  "module": "./dist/esm/src/index.js",
  "typings": "./dist/esm/src/index.d.ts",
  "scripts": {
    "clean:all": "npm run clean && npm cache clean --force && rimraf -- node_modules doc coverage dist  'src/**/*.js' 'src/**/*.js.map' 'test/**/*.js' 'test/**/*.js.map' 'tools/**/*.js' 'tools/**/*.js.map'",
    "clean": "npm run rimraf -- bundles",
    "rimraf": "rimraf",
    "start": "concurrently --raw \"webpack-dev-server --open\" \"npm run test:watch\"",
    "build:demo": "webpack --config webpack.config.demo.ts",
    "build:umd": "webpack --config webpack.config.umd.ts",
    "build:ngc": "ngc -p tsconfig-ngc.json",
    "build:dist": "npm run build:umd && npm run build:ngc",
    "build:clean": "del-cli dist",
    "test": "karma start --single-run && npm run build:dist && npm run build:clean",
    "test:watch": "karma start --auto-watch",
    "commit": "git-cz",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableGraph --disableCoverage --disablePrivateOrInternalSupport",
    "gh-pages": "git checkout gh-pages && git merge master --no-edit --no-ff && npm run build:demo && npm run compodoc && git add . && git commit -m \"chore: build demo and docs\" && git push && git checkout master",
    "copyfiles": "copyfiles package.json LICENSE README.md CHANGELOG.md dist",
    "prerelease": "npm test",
    "release:git": "git add package.json && git commit -m \"chore: bump version number\" && standard-version --first-release && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm run copyfiles && npm publish dist",
    "release": "npm run release:git && npm run release:npm",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "commitmsg": "validate-commit-msg",
    "codecov": "cat coverage/lcov.info | codecov -t de8f9e9e-8db7-42d6-86fd-06b0fcaeac64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sijpesteijn/ngx-delete-confirm.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "delete",
    "confirm"
  ],
  "author": "gijs.sijpesteijn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sijpesteijn/ngx-delete-confirm/issues"
  },
  "homepage": "https://github.com/sijpesteijn/ngx-delete-confirm#readme",
