    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.20",
    "@types/sinon": "^2.3.3",
    "@types/sinon-chai": "^2.7.28",
    "@types/webpack": "^3.0.8",
    "awesome-typescript-loader": "^3.2.3",
    "chai": "^4.1.1",
    "chai-dom": "^1.5.0",
    "codecov": "^2.3.0",
    "codelyzer": "^3.1.2",
