    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "phantomjs-prebuilt": "^2.1.15",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.3",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "standard-version": "^4.2.0",
