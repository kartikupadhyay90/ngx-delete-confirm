    "core-js": "^2.5.0",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "del-cli": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
